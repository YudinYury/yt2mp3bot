{
    "extends": "@dsm/frontend-code-style/tsconfig-backend.json",
    "include": ["src", "test", "test.ts", "scripts", "db-generated.ts", "sqlTypes", "sqlTypesOverride"],
    "compilerOptions": {
      "noErrorTruncation": true,
      "target": "ES2020",
